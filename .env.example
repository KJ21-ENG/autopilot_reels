# Environment variable template (no secrets)
# Copy to .env.local for local development.

# Stripe (Epic 3+)
# Server-side secret key used for Stripe API calls
STRIPE_SECRET_KEY=
# Client-side publishable key for Checkout initialization
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
# Webhook signing secret for /api/stripe/webhook
STRIPE_WEBHOOK_SECRET=
# Default price/product identifiers for checkout
STRIPE_PRICE_ID=
STRIPE_PRODUCT_ID=

# Supabase (Epic 4+)
# Supabase project URL for client/server usage
NEXT_PUBLIC_SUPABASE_URL=
# Supabase anon key for client-side usage
NEXT_PUBLIC_SUPABASE_ANON_KEY=
# Service role key for server-side admin operations
SUPABASE_SERVICE_ROLE_KEY=
